<table class="table table-boldered">
  <div class="row">
    <thead>
      <tr>
        <td>男神</td>
        <td>薪资</td>
        <td>年龄</td>
        <td>城市</td>
        <td>刊登时间</td>
      </tr>
    </thead>
    <tbody>
      <% @husbands.each do |husband| %>
        <tr>
          <td>
            <%= link_to(husband.title, husband_path(husband)) %>
          </td>
          <td>
            <%= husband.salary %>
          </td>
          <td>
            <%= husband.age%>
          </td>
          <td><%= husband.city%></td>
          <td>
            <%= husband.created_at %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </div>
</table>
